<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProDom.MobileClient.Session.SecurityPage"
             Shell.NavBarIsVisible="False">
    <VerticalStackLayout
        Margin="10,20,10,10">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.5*"/>
                <ColumnDefinition Width="5*"/>
            </Grid.ColumnDefinitions>
            <ImageButton 
                Aspect="AspectFill"
                WidthRequest="25"
                HeightRequest="25"
                Source="btn_back.svg"
                Clicked="BackButton_Clicked"/>
            <Image
                Grid.Column="1"
                HeightRequest="25"
                WidthRequest="200"
                Aspect="AspectFill"
                Source="security_title.svg"/>


        </Grid>
    </VerticalStackLayout>
</ContentPage>